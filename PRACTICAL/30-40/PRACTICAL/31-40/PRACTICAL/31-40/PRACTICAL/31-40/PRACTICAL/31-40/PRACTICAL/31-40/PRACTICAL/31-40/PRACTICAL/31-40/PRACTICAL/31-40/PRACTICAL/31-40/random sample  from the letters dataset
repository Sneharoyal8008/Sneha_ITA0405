# Create a factor from a random sample of the LETTERS (A to Z)
set.seed(123)  # Set seed for reproducibility
random_letters <- sample(LETTERS, size = 10, replace = TRUE)

# Create a factor from the random sample
letters_factor <- factor(random_letters)

# Extract the levels of the factor (get unique values)
levels_of_factor <- levels(letters_factor)

# Extract the first 5 levels
first_5_levels <- head(levels_of_factor, 5)

# Display the first 5 levels
print(first_5_levels)
